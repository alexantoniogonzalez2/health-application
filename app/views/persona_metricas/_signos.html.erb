<% disabled = (tipo == 'edit' ) ? '' : 'disabled' %>
<div class="modal fade" id="modal-container-signos" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div id="modal-dialog-signos" class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
         <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h4 class="modal-title" id="myModalLabel">
          Signos vitales
        </h4>
      </div>
      <div class="modal-body">
        <div class="tabbable" >
          <ul class="nav nav-tabs">
            <li class="active">
              <a href="#panel-signos" data-toggle="tab">Ingresar datos</a>
            </li>
            <li>
              <a id="tabpanel-frec_car" href="#panel-frec_car" data-toggle="tab">Frecuencia cardiaca</a>
            </li>
            <li>
              <a id="tabpanel-frec_res" href="#panel-frec_res" data-toggle="tab">Frecuencia respitaroria</a>
            </li>
            <li>
              <a id="tabpanel-presion_sis" href="#panel-presion_sis" data-toggle="tab">Presión sistólica</a>
            </li>
            <li>
              <a id="tabpanel-presion_dias" href="#panel-presion_dias" data-toggle="tab">Presión diastólica</a>
            </li>
            <li>
              <a id="tabpanel-presion_am" href="#panel-presion_am" data-toggle="tab">Presión arterial media</a>
            </li>
            <li>
              <a id="tabpanel-temp" href="#panel-temp" data-toggle="tab">Temperatura</a>
            </li>
            <li>
              <a id="tabpanel-sat" href="#panel-sat" data-toggle="tab">Saturación</a>
            </li>
          </ul>
          <div class="tab-content">
            <div class="tab-pane active" id="panel-signos">   
            <br><br>         
              <table>
                <tr>
                  <th>Signo vital</th>
                  <th>Valor</th>
                  <th>Unidad</th>
                  <th>Característica</th>
                </tr>
                <tr>
                  <td><span>Frecuencia cardiaca</span></td>
                  <td><input id="frec_car-<%= id %>" type="text" class="form-control metrica" value="<%= p_fc %>" maxlength="4" size="4" <%= disabled %> /></td>
                  <td><span class="text-med">latidos por minuto</span></td>
                  <td><input id="car_frec_car-<%= id %>" type="text" class="form-control metrica" value="<%= car_p_fc %>" maxlength="60" size="60" <%= disabled %> /></td>
                </tr>
                <tr>
                  <td><span>Frecuencia respiratoria</span></td>
                  <td><input id="frec_res-<%= id %>" type="text" class="form-control metrica" value="<%= p_fr %>" maxlength="4" size="4" <%= disabled %> /></td>
                  <td><span class="text-med">respiraciones por minuto</span></td>
                  <td><input id="car_frec_res-<%= id %>" type="text" class="form-control metrica" value="<%= car_p_fr %>" maxlength="60" size="60" <%= disabled %> /></td>
                </tr>
                <tr>
                  <td><span>Presión sistólica</span></td>
                  <td><input id="presion-sis-<%= id %>" type="text" class="form-control medicamento" value="<%= p_pr_sis %>" maxlength="4" size="4" onkeyup="calcularPAM(<%= id %>)" <%= disabled %> /></td>
                  <td><span class="text-med">mm Hg</span></td> 
                  <td><input id="car_presion-sis-<%= id %>" type="text" class="form-control metrica" value="<%= car_p_pr_sis %>" maxlength="60" size="60" <%= disabled %> /></td>
                </tr>
                <tr>
                  <td><span>Presión diastólica</span></td>
                  <td><input id="presion-dias-<%= id %>" type="text" class="form-control medicamento" value="<%= p_pr_dias %>" maxlength="4" size="4" onkeyup="calcularPAM(<%= id %>)" <%= disabled %> /></td>
                  <td><span class="text-med">mm Hg</span></td>
                  <td><input id="car_presion-dias-<%= id %>" type="text" class="form-control metrica" value="<%= car_p_pr_dias %>" maxlength="60" size="60" <%= disabled %> /></td>
                </tr>
                <tr>
                  <td><span>Presión arterial media</span></td>
                  <td><input disabled id="presion-am-<%= id %>" type="text" class="form-control medicamento" value="<%= p_pr_am %>" maxlength="4" size="4" <%= disabled %> /></td>
                  <td><span class="text-med">mm Hg</span></td>
                  <td><input id="car_presion-am-<%= id %>" type="text" class="form-control metrica" value="<%= car_p_pr_am %>" maxlength="60" size="60" <%= disabled %> /></td>
                </tr>
                <tr>
                  <td><span>Temperatura</span></td>
                  <td><input id="temp-<%= id %>" type="text" class="form-control metrica" value="<%= p_t %>" maxlength="4" size="4" <%= disabled %> /></td>
                  <td><span class="text-med">C° (ej.: 36.5)</span></td>
                  <td><input id="car_temp-<%= id %>" type="text" class="form-control metrica" value="<%= car_p_t %>" maxlength="60" size="60" <%= disabled %> /></td>
                </tr>
                <tr>
                  <td><span>Saturación</span></td>
                  <td><input id="sat-<%= id %>" type="text" class="form-control metrica" value="<%= p_sat %>" maxlength="4" size="4" <%= disabled %> /></td>
                  <td><span class="text-med">%</span></td>
                  <td><input id="car_sat-<%= id %>" type="text" class="form-control metrica" value="<%= car_p_sat %>" maxlength="60" size="60" <%= disabled %> /></td>
                </tr>
              </table>              
            </div>
            <div class="tab-pane" id="panel-frec_car">              
              <div id="grafico_frec_car" class="metricas" ></div>         
            </div>
            <div class="tab-pane" id="panel-frec_res">              
              <div id="grafico_frec_res" class="metricas"></div>            
            </div>
            <div class="tab-pane" id="panel-presion_sis">              
              <div id="grafico_presion_sis" class="metricas"></div>            
            </div>
            <div class="tab-pane" id="panel-presion_dias">              
              <div id="grafico_presion_dias" class="metricas"></div>            
            </div>
            <div class="tab-pane" id="panel-presion_am">              
              <div id="grafico_presion_am" class="metricas"></div>            
            </div>
            <div class="tab-pane" id="panel-temp">              
              <div id="grafico_temp" class="metricas"></div>            
            </div>
            <div class="tab-pane" id="panel-sat">              
              <div id="grafico_sat" class="metricas"></div>            
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar ventana</button>
        <% if tipo == 'edit' %>
          <button type="button" onclick="guardarSignos(<%= id %>)" class="btn btn-success">Guardar signos vitales</button>
        <% end %>  
      </div>
    </div>      
  </div>    
</div>

