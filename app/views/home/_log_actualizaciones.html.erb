<div class="panel panel-success">
  <div class="panel-heading">
    <h3 class="panel-title">Ãšltimos eventos</h3>
  </div>
  <div class="panel-body">
    <table class="table table-striped table-bordered table-hover table-condensed">
      <thead>
        <tr><th>Paciente</th><th>Hora</th><th>Evento</th><th>Modificado por</th></tr>
      </thead>
      <tbody>
        <% @actualizaciones.each do |actualizacion| %>
          <tr>
            <td>            <% if actualizacion.persona %>
              <%= actualizacion.persona.showName('%n%p%m') %>
            <% else %>
              No agendada.
            <% end %></td>
            <td><%= actualizacion.fecha %></td>          
            <td><% %></td> 
            <td> <% if actualizacion.agendamiento_log_estados.getResponsable %>
              <%= actualizacion.agendamiento_log_estados.getResponsable.showName('%n%p%m') %>
            <% else %>
              No agendada.
            <% end %></td> 
          </tr>
        <% end %>  
      </tbody>
    </table>
   
  </div>
  <div class="panel-footer">
    Panel footer
  </div>
</div>

