<tr id="accion_masiva<%= accion_masiva.id %>"> 
	<td>Horas creadas</td>
	<td><%= accion_masiva.created_at.strftime("%Y-%m-%d %H:%M") %> </td>
	<td><%= accion_masiva.especialidad_prestador_profesional.profesional.showName('%n%p%m') %></td>
	<td><%= accion_masiva.especialidad_prestador_profesional.especialidad.nombre %></td>
	<td><%= accion_masiva.total_agendamientos %></td>
	<td>-</td>
	<td><a href="#cancelar_accion" role="button" class="btn" data-toggle="modal" onclick="cargarCancelarAccion(<%= accion_masiva.id %>)">Deshacer</a></td>
</tr>