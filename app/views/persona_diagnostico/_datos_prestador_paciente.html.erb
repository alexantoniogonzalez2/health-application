			<div class="col-md-4 column">
				<label>Rut</label>
			</div>
			<div class="col-md-8 column">:
				<%= @agendamiento.persona.showRut %>
			</div>
		</div>
		<div class="row clearfix">	
			<div class="col-md-4 column">
				<label>Edad</label>
			</div>
			<div class="col-md-8 column">:
				<%= @agendamiento.persona.age_text %>
			</div>
		</div>
		<div class="row clearfix">	
			<div class="col-md-4 column">
				<label>Sexo</label>
			</div>
			<div class="col-md-8 column">:
