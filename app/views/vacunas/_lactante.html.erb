<h3>Seguimiento</h3>
<div class="container">
  <div class="row clearfix">
    <div class="col-md-12 column">
      <table class="table table-striped table-hover table-bordered">
        <thead><tr><th>Edad</th><th>Vacuna</th><th>Estado</th></tr></thead>
        <tbody>
          <% @calendario_vacunas_lactante.each do |calendario_vacuna| %>
              <tr>
                <td><%= calendario_vacuna.edad %></td>
                <td><%= calendario_vacuna.vacuna.nombre %></td>
                <td><label class="calendario-vacunas" for="checkboxes-0"><input type="checkbox" id="checkboxes-vac-<%= calendario_vacuna.id %>"></label></td>
              </tr>
          <% end %>
         </tbody>    
      </table>      
    </div>
  </div>
</div>