<h3>Enfermedades inmunoprevenibles según campaña, urgencia o epidemiologia</h3>
<div class="container">
	<div class="row clearfix">
		<div class="col-md-12 column">
			<table class="table table-striped table-hover table-bordered">
			  <thead><tr class="info"><th>Vacuna</th><th>Tipo</th><th>Protege contra</th></tr></thead>
			  <tbody>
					<% @otras_vacunas.each do |vacuna| %>
						<tr><td><%= vacuna['nombre'] %></td><td><%= vacuna['tipo'] %></td><td><%= vacuna['protege_contra'] %></td></tr>
					<% end %>
				</tbody>	
			</table>		
		</div>
	</div>
</div>